def знайти_мінімальний_додатній_елемент(масив):
    мінімальний_додатній_елемент = None
    
    for елемент in масив:
        if елемент > 0:  # Перевіряємо, чи елемент є додатнім
            if мінімальний_додатній_елемент is None or елемент < мінімальний_додатній_елемент:
                мінімальний_додатній_елемент = елемент

    return мінімальний_додатній_елемент

# Отримання масиву від користувача
N = int(input("Введіть кількість елементів у масиві: "))
масив = []
for i in range(N):
    елемент = float(input(f"Введіть {i+1}-й елемент: "))
    масив.append(елемент)

# Знаходження мінімального додатнього елементу
результат = знайти_мінімальний_додатній_елемент(масив)

if результат is not None:
    print(f"Мінімальний додатній елемент: {результат}")
else:
    print("У масиві немає додатніх елементів.")
